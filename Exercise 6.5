def ack(m, n):
    if m < 0 or n < 0:
        print("Ackermann function is not defined for negative integers")
        return None
    elif m == 0:
        return n+1
    elif n == 0:
        return ack(m-1, 1)
    elif n > 0:
        return ack(m-1, ack(m, n-1))


x = 2
y = -1
result = ack(x, y)
print(result)
